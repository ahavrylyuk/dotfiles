# This is Git's per-user configuration file.
[user]
name = Andriy Havrylyuk
email = andriy.havrylyuk@gmail.com

[diff]
  tool = wstorm
[difftool]
  prompt = false
[difftool.wstorm]
  cmd = /usr/local/bin/wstorm diff "$LOCAL" "$REMOTE"
[merge]
  tool = wstorm
[mergetool.wstorm]
  cmd = /usr/local/bin/wstorm merge "$LOCAL" "$REMOTE" "$BASE" "$MERGED"


[core]
	editor = vim
[push]
	default = simple
